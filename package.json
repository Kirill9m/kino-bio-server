{
  "name": "kino-bio-projekt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "sass": "sass --watch src/styles/styles.scss ./src/styles/main.css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2"
  },
  "dependencies": {
    "express": "^4.21.2",
    "sass": "^1.83.4"
  }
}
